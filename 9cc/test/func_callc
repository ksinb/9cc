int foo1(int a) {
  return a;
}

int foo2(int a, int b) {
  return a + b;
}

int foo3(int a, int b, int c) {
  return a + b + c;
}

int foo4(int a, int b, int c, int d) {
  return a + b + c + d;
}

int foo5(int a, int b, int c, int d, int e) {
  return a + b + c + d + e;
}

int foo6(int a, int b, int c, int d, int e, int f) {
  return a + b + c + d + e + f;
}